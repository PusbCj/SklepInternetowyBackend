<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.ehcache.org/v3"
        xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">
    <cache-template name="default">
        <expiry>
            <ttl unit="seconds">60</ttl>
        </expiry>
        <heap unit="entries">1000</heap>
    </cache-template>

    <cache alias="AllProductByCategory" uses-template="default"/>

    <cache alias="AllProductByCategoryDesc" uses-template="default"/>

    <cache alias="AllProductByCategoryWithoutParam" uses-template="default"/>

    <cache alias="AllProductByCategoryMultiParam" uses-template="default"/>

    <cache alias="GetOneProductById" uses-template="default"/>

    <cache alias="GetAllBrandsByCategory" uses-template="default"/>

    <cache alias="GetAllCategoryAge" uses-template="default"/>

    <cache alias="GetAllBrands" uses-template="default"/>

</config>
